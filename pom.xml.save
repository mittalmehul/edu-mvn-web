<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.sample.webproject</groupId>
  <artifactId>SampleWebApp</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>SampleWebApp Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <!-- https://mvnrepository.com/artifact/org.apache.tomcat/tomcat -->
	<dependency>
    		<groupId>org.apache.tomcat</groupId>
    		<artifactId>tomcat</artifactId>
    		<version>10.0.0-M1</version>
    		<type>pom</type>
	</dependency>
</dependencies>
<distributionManagement>
    <repository>
      <id>sample-web</id>
      <name>Releases</name>
      <url>http://localhost:8181/repository/maven-releases</url>
    </repository>
    <snapshotRepository>
      <id>sample-web</id>
      <name>Snapshot</name>
      <url>http://localhost:8181/repository/maven-snapshots</url>
    </snapshotRepository>
</distributionManagement>
  <build>
    <finalName>SampleWebApp</finalName>
  </build>
</project>
